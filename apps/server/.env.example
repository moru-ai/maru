# Database
DATABASE_URL="postgresql://user:password@host:port/database"

# GitHub OAuth
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# Moru Sandbox Configuration
MORU_API_KEY=moru_your_api_key
MORU_TEMPLATE_ID=claude-agent
MORU_SANDBOX_TIMEOUT_MS=3600000

# Anthropic
ANTHROPIC_API_KEY=sk-ant-api03-your_key

# GCS Storage (for workspace persistence)
GCS_BUCKET_NAME=your-bucket-name
# Option 1: Service account key file
GCS_KEY_FILE=./gcs-key.json
# Option 2: Base64-encoded credentials (for env vars)
# GCS_CREDENTIALS_BASE64=base64_encoded_service_account_json
